model payment {
    id          Int      @id @default(autoincrement())
    payment_id  String
    status      String
    amount      String
    currency    String
    service_Fee String
    name        String
    email       String
    created_At  DateTime
    platform    String?
    user_id     Int
    user        User     @relation(fields: [user_id], references: [id])
}
